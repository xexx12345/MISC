    "\n",
    "#Euler 2\n",
    "\n",
    "L = []\n",
    "def fib(n):\n",
    "    a = 1\n",
    "    b = 2\n",
    "    while a < n:\n",
    "        L.append(a)\n",
    "        a,b = b, a + b\n",
    "        \n",
    "        \n",
    "def fib1(n):\n",
    "    a = 1\n",
    "    b = 2\n",
    "    while a < n:\n",
    "        yield a\n",
    "        a,b = b, a+ b\n"
   ]
